generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  password String 
  status Int
  id_profile Int
}


model Cultura {
  id    Int     @id @default(autoincrement())
  name  String?
  status Int @default(1)
}